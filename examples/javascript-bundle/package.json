{
  "name": "toolvault-javascript-bundle",
  "version": "1.0.0",
  "description": "Mock JavaScript tool bundle for ToolVault UI testing",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "tools/**/*.js"
    ]
  }
}
