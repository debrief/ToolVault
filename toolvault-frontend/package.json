{
  "name": "toolvault-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "kill-vite": "ps aux | grep '[v]ite' | awk '{print $2}' | xargs kill -9 || true",
    "lint": "eslint . --max-warnings=0",
