{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm generate-types && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test performance.spec.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "generate-types": "node scripts/generate-types.cjs",
    "prebuild": "pnpm generate-types",
    "analyze": "npx vite-bundle-analyzer dist",
    "size-limit": "bundlesize",
    "lighthouse": "lhci autorun",
    "lighthouse:desktop": "lhci autorun --config=lighthouserc.js",
    "lighthouse:mobile": "lhci autorun --preset=mobile"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-data-grid": "^8.10.0",
    "@tanstack/react-query": "^5.85.3",
    "@types/flexsearch": "^0.7.42",
    "@types/leaflet": "^1.9.20",
    "@types/prismjs": "^1.26.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "async-retry": "^1.3.3",
    "chart.js": "^4.5.0",
    "flexsearch": "^0.8.205",
    "html2canvas": "^1.4.1",
    "leaflet": "^1.9.4",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.8.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-virtuoso": "^4.14.0",
    "react-window": "^1.8.8",
    "web-vitals": "^4.2.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.54.2",
    "@tanstack/react-query-devtools": "^5.85.3",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/async-retry": "^1.4.9",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.0.0",
    "bundlesize": "^0.18.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "json-schema-to-typescript": "^15.0.4",
    "msw": "^2.10.5",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vite-bundle-analyzer": "^0.12.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "bundlesize": [
    {
      "path": "./dist/assets/*.js",
      "maxSize": "500kb",
      "compression": "gzip"
    },
    {
      "path": "./dist/assets/*.css",
      "maxSize": "100kb",
      "compression": "gzip"
    }
  ],
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}